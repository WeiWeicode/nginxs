utton type=\"button\" class=\"btn btn-primary\" @click=\"goHome\">返回首頁</button>\r\n        </div>\r\n        </div>\r\n        \r\n    \r\n\r\n    <div v-show=\"shoppingCartBoolean\">\r\n\r\n        <div>\r\n            <table class=\"table table-striped table-bordered\">\r\n                <thead>\r\n                    <tr>\r\n                        <th>商品名稱</th>\r\n                        <th>商品價格</th>\r\n                    </tr>\r\n                </thead>\r\n                <tbody>\r\n                    <tr v-for=\"item in ProductsTable\" :key=\"item.id\">\r\n                        <td>{{ item.title }}</td>\r\n                        <td>{{ item.price }}</td>\r\n                    </tr>\r\n                </tbody>\r\n            </table>\r\n        </div>\r\n\r\n\r\n        <div class=\"form-group\">\r\n            <div class=\"alert alert-success\">\r\n            <label for=\"total\">總金額:{{ord_total}}</label>\r\n        </div>\r\n        </div>\r\n\r\n        <div class=\"form-group\">\r\n            <label for=\"byusername\">收件人</label>\r\n            <input type=\"text\" class=\"form-control\" id=\"byusername\" placeholder=\"請輸入收件人\" v-model=\"ord_byusername\">\r\n        </div>\r\n\r\n        <div class=\"form-group\">\r\n            <label for=\"phone\">電話</label>\r\n            <input type=\"tel\" class=\"form-control\" id=\"phone\" placeholder=\"請輸入電話\" v-model=\"ord_phone\">\r\n        </div>\r\n\r\n        <div class=\"form-group\">\r\n            <label for=\"address\">地址</label>\r\n            <input type=\"text\" class=\"form-control\" id=\"address\" placeholder=\"請輸入地址\" v-model=\"ord_address\">\r\n        </div>\r\n\r\n        <div class=\"form-group\">\r\n            <!-- 單選式付款方式(radio) 信用卡,ATM,貨到付款-->\r\n            <label for=\"payment\">付款方式</label>\r\n            <div class=\"form-check\">\r\n                <input class=\"form-check-input\" type=\"radio\" name=\"payment\" id=\"payment1\" value=\"信用卡\"\r\n                    v-model=\"ord_payment\">\r\n                <label class=\"form-check-label\" for=\"payment1\">\r\n                    信用卡\r\n                </label>\r\n            </div>\r\n            <div class=\"form-check\">\r\n                <input class=\"form-check-input\" type=\"radio\" name=\"payment\" id=\"payment2\" value=\"ATM\"\r\n                    v-model=\"ord_payment\">\r\n                <label class=\"form-check-label\" for=\"payment2\">\r\n                    ATM\r\n                </label>\r\n            </div>\r\n            <div class=\"form-check\">\r\n                <input class=\"form-check-input\" type=\"radio\" name=\"payment\" id=\"payment3\" value=\"貨到付款\"\r\n                    v-model=\"ord_payment\">\r\n                <label class=\"form-check-label\" for=\"payment3\">\r\n                    貨到付款\r\n                </label>\r\n            </div>\r\n        </div>\r\n        <!-- 付款按鈕 -->\r\n        <button type=\"submit\" class=\"btn btn-primary\" @click=\"Paying\">付款</button>\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n    </div>\r\n</div>\r\n\r\n</template>\r\n\r\n\r\n<script>\r\n\r\nimport axios from \"axios\";\r\n\r\n\r\nexport default {\r\n    \r\n    data() {\r\n        return {\r\n            // isShow: true\r\n            ProductsTable: [],\r\n\r\n            ord_total: '',\r\n            ord_phone: '',\r\n            ord_address: '',\r\n            ord_payment: '',\r\n            ord_byusername: '',\r\n            ord_byid: '',\r\n            ord_state: '',\r\n\r\n            tempres: [],\r\n            tempOrder: [],\r\n            tempShppingCart: [],\r\n\r\n        }\r\n\r\n    },\r\n\r\n    mounted: function () {\r\n        let ProductsTable = JSON.parse(localStorage.getItem('ShoppingCart'))\r\n        this.ProductsTable = ProductsTable\r\n        // 取得localStorage資料\r\n        this.ord_total = this.ProductsTable.reduce((total, item) => total + item.price, 0)\r\n        // 計算總金額\r\n        let user = JSON.parse(localStorage.getItem('user'))\r\n        this.ord_byusername = user.username\r\n        this.ord_byid = user.id\r\n        // byid: 取得localStorage user裡的id\r\n    },\r\n\r\n    methods: {\r\n        goHome() {\r\n            this.$router.push('/')\r\n        },\r\n\r\n\r\n        Paying() {\r\n            \r\n            // 將local資料轉成json格式\r\n\r\n            let order = {\r\n                ord_total: this.ord_total,\r\n                ord_phone: this.ord_phone,\r\n                ord_address: this.ord_address,\r\n                ord_payment: this.ord_payment,\r\n                ord_byusername: this.ord_byusername,\r\n                ord_byid: this.ord_byid,\r\n                ord_state: this.ord_state,\r\n            }\r\n            console.log(order)\r\n            // 將local資料傳到orders\r\n            \r\n            axios.post('http://192.168.68.60:8082/api/products/addOrder', order)\r\n                .then((res) => {\r\n                    console.log(res)\r\n                    this.tempres = res.data\r\n                    localStorage.setItem('order', JSON.stringify(this.tempres))\r\n                    // 取得後端回傳的訂單id\r\n                    // axios.get('http://192.168.68.60:8082/api/products/allOrders')\r\n                    //     .then((res) => {\r\n                    //         console.log(res)\r\n                    //         // 將local資料傳到orderproducts,要對應到訂單id\r\n                    //         axios.post('http://192.168.68.60:8082/api/products/addOrderProduct/' + res.data.id)\r\n                    //             .then((res) => {\r\n                    //                 console.log(res)\r\n                    //             })\r\n                    //             .catch((err) => {\r\n                    //                 console.log(err)\r\n                    //             })\r\n                    //     })\r\n                    //     .catch((err) => {\r\n                    //         console.log(err)\r\n                    //     })\r\n                })\r\n                .catch((err) => {\r\n                    console.log(err)\r\n                })\r\n\r\n        // this.tempOrder = JSON.parse(localStorage.getItem('tempres'))\r\n        // this.tempShppingCart = JSON.parse(localStorage.getItem('ShoppingCart'))\r\n        // let orderproducts = {\r\n\r\n        // }\r\n\r\n        // 取出localStorage tempres的Data資料\r\n        \r\n        // 付款按鈕 (總共5秒處理)\r\n        // 1.將local資料傳到orders後端 (延遲一秒) (axios post orders http://192.168.68.60:8082/api/products/addOrder)\r\n        // 2.取得後端回傳的訂單id (axios get orders http://192.168.68.60:8082/api/products/allOrders)(條件match:createdAt(建立日期) = new Date(年月時分)與 ord_byid(使用者id)==user.id)\r\n        // 3.將local資料傳到orderproducts,要對應到訂單id (axios post orderproducts http://192.168.68.60:8082/api/products/addOrderProduct/:id)\r\n        // 4.將local資料刪除\r\n        // 5.畫面跳轉到訂單頁面\r\n    }\r\n\r\n    },\r\n\r\n\r\n    computed: {\r\n        shoppingCartBoolean() {\r\n\r\n\r\n\r\n            if (localStorage.getItem('ShoppingCart') == null) {\r\n                return false\r\n            } else {\r\n                return true\r\n            }\r\n\r\n            // 判斷localStorage ShopoingCart是否有資料 \r\n        }\r\n\r\n    },\r\n\r\n\r\n}\r\n\r\n</script>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}