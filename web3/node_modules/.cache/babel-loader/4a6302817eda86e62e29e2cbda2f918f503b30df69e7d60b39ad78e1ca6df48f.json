{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from \"axios\";\nexport default {\n  data() {\n    return {\n      // isShow: true\n      ProductsTable: [],\n      ord_total: '',\n      ord_phone: '',\n      ord_address: '',\n      ord_payment: '',\n      ord_byusername: '',\n      ord_byid: '',\n      ord_state: '',\n      tempres: [],\n      tempOrder: [],\n      tempShppingCart: []\n    };\n  },\n  mounted: function () {\n    let ProductsTable = JSON.parse(localStorage.getItem('ShoppingCart'));\n    this.ProductsTable = ProductsTable;\n    // 取得localStorage資料\n    this.ord_total = this.ProductsTable.reduce((total, item) => total + item.price, 0);\n    // 計算總金額\n    let user = JSON.parse(localStorage.getItem('user'));\n    this.ord_byusername = user.username;\n    this.ord_byid = user.id;\n    // byid: 取得localStorage user裡的id\n  },\n\n  methods: {\n    goHome() {\n      this.$router.push('/');\n    },\n    Paying() {\n      // 將local資料轉成json格式\n\n      let order = {\n        ord_total: this.ord_total,\n        ord_phone: this.ord_phone,\n        ord_address: this.ord_address,\n        ord_payment: this.ord_payment,\n        ord_byusername: this.ord_byusername,\n        ord_byid: this.ord_byid,\n        ord_state: this.ord_state\n      };\n      console.log(order);\n      // 將local資料傳到orders\n\n      axios.post('http://192.168.68.60:8082/api/products/addOrder', order).then(res => {\n        console.log(res);\n        this.tempres = res.data;\n        localStorage.setItem('order', JSON.stringify(this.tempres));\n        // 取得後端回傳的訂單id\n        // axios.get('http://192.168.68.60:8082/api/products/allOrders')\n        //     .then((res) => {\n        //         console.log(res)\n        //         // 將local資料傳到orderproducts,要對應到訂單id\n        //         axios.post('http://192.168.68.60:8082/api/products/addOrderProduct/' + res.data.id)\n        //             .then((res) => {\n        //                 console.log(res)\n        //             })\n        //             .catch((err) => {\n        //                 console.log(err)\n        //             })\n        //     })\n        //     .catch((err) => {\n        //         console.log(err)\n        //     })\n      }).catch(err => {\n        console.log(err);\n      });\n\n      // this.tempOrder = JSON.parse(localStorage.getItem('tempres'))\n      // this.tempShppingCart = JSON.parse(localStorage.getItem('ShoppingCart'))\n      // let orderproducts = {\n\n      // }\n\n      // 取出localStorage tempres的Data資料\n\n      // 付款按鈕 (總共5秒處理)\n      // 1.將local資料傳到orders後端 (延遲一秒) (axios post orders http://192.168.68.60:8082/api/products/addOrder)\n      // 2.取得後端回傳的訂單id (axios get orders http://192.168.68.60:8082/api/products/allOrders)(條件match:createdAt(建立日期) = new Date(年月時分)與 ord_byid(使用者id)==user.id)\n      // 3.將local資料傳到orderproducts,要對應到訂單id (axios post orderproducts http://192.168.68.60:8082/api/products/addOrderProduct/:id)\n      // 4.將local資料刪除\n      // 5.畫面跳轉到訂單頁面\n    }\n  },\n\n  computed: {\n    shoppingCartBoolean() {\n      if (localStorage.getItem('ShoppingCart') == null) {\n        return false;\n      } else {\n        return true;\n      }\n\n      // 判斷localStorage ShopoingCart是否有資料 \n    }\n  }\n};","map":{"version":3,"mappings":";AAwGA,OAAOA,KAAI,MAAO,OAAO;AAGzB,eAAe;EAEXC,IAAI,GAAG;IACH,OAAO;MACH;MACAC,aAAa,EAAE,EAAE;MAEjBC,SAAS,EAAE,EAAE;MACbC,SAAS,EAAE,EAAE;MACbC,WAAW,EAAE,EAAE;MACfC,WAAW,EAAE,EAAE;MACfC,cAAc,EAAE,EAAE;MAClBC,QAAQ,EAAE,EAAE;MACZC,SAAS,EAAE,EAAE;MAEbC,OAAO,EAAE,EAAE;MACXC,SAAS,EAAE,EAAE;MACbC,eAAe,EAAE;IAErB;EAEJ,CAAC;EAEDC,OAAO,EAAE,YAAY;IACjB,IAAIX,aAAY,GAAIY,IAAI,CAACC,KAAK,CAACC,YAAY,CAACC,OAAO,CAAC,cAAc,CAAC;IACnE,IAAI,CAACf,aAAY,GAAIA,aAAY;IACjC;IACA,IAAI,CAACC,SAAQ,GAAI,IAAI,CAACD,aAAa,CAACgB,MAAM,CAAC,CAACC,KAAK,EAAEC,IAAI,KAAKD,KAAI,GAAIC,IAAI,CAACC,KAAK,EAAE,CAAC;IACjF;IACA,IAAIC,IAAG,GAAIR,IAAI,CAACC,KAAK,CAACC,YAAY,CAACC,OAAO,CAAC,MAAM,CAAC;IAClD,IAAI,CAACV,cAAa,GAAIe,IAAI,CAACC,QAAO;IAClC,IAAI,CA