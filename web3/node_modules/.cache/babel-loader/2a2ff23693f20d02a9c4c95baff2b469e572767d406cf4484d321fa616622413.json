e: {\r\n            get() {\r\n                return this.$store.state.image;\r\n            },\r\n            set(value) {\r\n                this.$store.commit('setImage', value);\r\n            }\r\n        },\r\n        // 產品上架\r\n        published: {\r\n            get() {\r\n                return this.$store.state.published;\r\n            },\r\n            set(value) {\r\n                this.$store.commit('setPublished', value);\r\n            }\r\n        },\r\n    },\r\n\r\n\r\n\r\n}\r\n\r\n</script>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}