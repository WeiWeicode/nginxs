</form>\r\n\r\n    <button type=\"submit\" class=\"btn btn-primary\" @click=\"addproducts\" >新增產品</button>\r\n\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios';\r\n\r\nexport default {\r\n    data() {\r\n        return {\r\n            // 產品名稱\r\n            title: '',\r\n            // 產品金額\r\n            price: '',\r\n            // 產品描述\r\n            description: '',\r\n            // 產品照片\r\n            image: '',\r\n            // 產品上架\r\n            published: false,\r\n        }\r\n    },\r\n    methods: {\r\n        // 新增產品 formdata\r\n        uploadImage(event) {\r\n            this.image = event.target.files[0];\r\n        },\r\n\r\n        addproducts() {\r\n            let formData = new FormData();\r\n            formData.append('title', this.title);\r\n            formData.append('price', this.price);\r\n            formData.append('description', this.description);\r\n            formData.append('image', this.image);\r\n            formData.append('published', this.published);\r\n\r\n\r\n            axios.post('http://192.168.68.60:8082/api/products/addProduct', formData).then((res) => {\r\n                console.log(res)\r\n                alert('新增成功');\r\n                \r\n                \r\n                \r\n                // 上傳成功跳出提示\r\n\r\n            }).catch((err) => {\r\n                console.log(err);\r\n            })\r\n        }\r\n    }\r\n}\r\n\r\n\r\n\r\n</script>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}