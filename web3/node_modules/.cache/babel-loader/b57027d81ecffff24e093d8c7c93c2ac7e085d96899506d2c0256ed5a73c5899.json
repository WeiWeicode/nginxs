push('/showproducts');\n    },\n\n    // toupdataproducts\n    toupdataproducts() {\n      this.$router.push('/updataproducts');\n    },\n\n  }\n};\n</script>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}