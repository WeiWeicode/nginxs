{"ast":null,"code":"import axios from 'axios';\nexport default {\n  // 子接收父傳值\n  props: ['accept'],\n  data() {\n    return {\n      // // 產品名稱\n      // title: '',\n      // // 產品金額\n      // price: '',\n      // // 產品描述\n      // description: '',\n      // // 產品照片\n      // image: '',\n      // // 產品上架\n      // published: false,\n\n      localProducts: []\n    };\n  },\n  mounted: function () {\n    // 取得localStorage item\n    this.localProducts = JSON.parse(localStorage.getItem('item'));\n    console.log(this.localProducts);\n  },\n  methods: {\n    // 新增產品 formdata\n    uploadImage(event) {\n      this.image = event.target.files[0];\n    },\n    addproducts() {\n      let formData = new FormData();\n      formData.append('title', this.title);\n      formData.append('price', this.price);\n      formData.append('description', this.description);\n      formData.append('image', this.image);\n      formData.append('published', this.published);\n      axios.patch('http://192.168.68.60:8082/api/products/addProduct', formData).then(res => {\n        console.log(res);\n        alert('新增成功');\n        // this.$router.push('/addproducts');\n\n        this.title = '';\n        this.price = '';\n        this.description = '';\n        this.published = false;\n        // 上傳成功跳出提示, 畫面重新整理\n      }).catch(err => {\n        console.log(err);\n      });\n    }\n  },\n  computed: {\n    // 取得本地端item.title\n  }\n};","map":{"version":3,"mappings":"AA2DA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACX;EACAC,KAAK,EAAE,CAAC,QAAQ,CAAC;EACjBC,IAAI,GAAG;IACH,OAAO;MACH;MACA;MACA;MACA;MACA;MACA;MACA;MACA;MACA;MACA;;MAEAC,aAAa,EAAE;IAEnB;EAEJ,CAAC;EAEDC,OAAO,EACH,YAAY;IACR;IACA,IAAI,CAACD,aAAY,GAAIE,IAAI,CAACC,KAAK,CAACC,YAAY,CAACC,OAAO,CAAC,MAAM,CAAC,CAAC;IAC7DC,OAAO,CAACC,GAAG,CAAC,IAAI,CAACP,aAAa,CAAC;EAEnC,CAAC;EAELQ,OAAO,EAAE;IACL;IACAC,WAAW,CAACC,KAAK,EAAE;MACf,IAAI,CAACC,KAAI,GAAID,KAAK,CAACE,MAAM,CAACC,KAAK,CAAC,CAAC,CAAC;IACtC,CAAC;IAEDC,WAAW,GAAG;MACV,IAAIC,QAAO,GAAI,IAAIC,QAAQ,EAAE;MAC7BD,Q