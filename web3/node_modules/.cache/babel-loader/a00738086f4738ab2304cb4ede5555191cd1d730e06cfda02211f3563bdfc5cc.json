\n        <!-- <div class=\"form-group\">\r\n            <label for=\"exampleFormControlFile1\">上傳照片</label>\r\n            <input type=\"file\" class=\"form-control-file\" id=\"exampleFormControlFile1\">\r\n        </div> -->\r\n\r\n        <!-- 照片上傳 -->\r\n        <div class=\"form-group\">\r\n            <label for=\"exampleFormControlFile1\">上傳照片</label>\r\n            <input type=\"file\" class=\"form-control-file\" id=\"exampleFormControlFile1\" @change=\"uploadImage\">\r\n        </div>\r\n\r\n        <!-- 產品是否上架 -->\r\n\r\n\r\n\r\n        <div class=\"custom-control custom-checkbox mb-3\">\r\n            <input type=\"checkbox\" class=\"custom-control-input\" id=\"customControlValidation1\" required\r\n                v-model=\"published\">\r\n            <label class=\"custom-control-label\" for=\"customControlValidation1\">是否上架產品</label>\r\n        </div>\r\n\r\n    </form>\r\n\r\n    <button type=\"submit\" class=\"btn btn-primary\" @click=\"updateproducts\">更新產品</button>\r\n\r\n\r\n    {{accept.id}}\r\n    {{accept.title}}\r\n    {{accept.price}}\r\n    {{accept.description}}\r\n    {{accept.image}}\r\n    {{accept.published}}\r\n\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios';\r\n\r\nexport default {\r\n    // 子接收父傳值\r\n    props: ['accept'],\r\n    data() {\r\n        return {\r\n            // // 產品名稱\r\n            // title: '',\r\n            // // 產品金額\r\n            // price: '',\r\n            // // 產品描述\r\n            // description: '',\r\n            // // 產品照片\r\n            // image: '',\r\n            // // 產品上架\r\n            // published: false,\r\n\r\n            localProducts: [],\r\n\r\n        }\r\n\r\n    },\r\n\r\n    mounted:\r\n        function () {\r\n            // 取得localStorage item\r\n            this.localProducts = JSON.parse(localStorage.getItem('item'));\r\n            console.log(this.localProducts);\r\n\r\n        },\r\n\r\n    methods: {\r\n        // 新增產品 formdata\r\n        uploadImage(event) {\r\n            this.image = event.target.files[0];\r\n        },\r\n\r\n        addproducts() {\r\n            let formData = new FormData();\r\n            formData.append('title', this.title);\r\n            formData.append('price', this.price);\r\n            formData.append('description', this.description);\r\n            formData.append('image', this.image);\r\n            formData.append('published', this.published);\r\n\r\n\r\n            axios.patch('http://192.168.68.60:8082/api/products/addProduct', formData).then((res) => {\r\n                console.log(res)\r\n                alert('新增成功')\r\n                // this.$router.push('/addproducts');\r\n\r\n                this.title = '';\r\n                this.price = '';\r\n                this.description = '';\r\n                this.published = false;\r\n                // 上傳成功跳出提示, 畫面重新整理\r\n\r\n            }).catch((err) => {\r\n                console.log(err);\r\n            })\r\n        }\r\n    },\r\n\r\n    computed: {\r\n        // 取得本地端item.title\r\n        \r\n\r\n\r\n    }\r\n\r\n\r\n}\r\n\r\n\r\n\r\n</script>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}