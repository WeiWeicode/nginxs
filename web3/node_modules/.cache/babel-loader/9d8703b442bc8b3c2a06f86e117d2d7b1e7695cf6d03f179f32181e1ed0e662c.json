roducts\")\r\n            // 發出取得請求\r\n            .then((response) => {\r\n                // 取得回應後的處理\r\n                console.log(response);\r\n                // 這裡的response是API回傳的資料\r\n                this.ProductsTable = response.data;\r\n            })\r\n            .catch((error) => {\r\n                // 發生錯誤時的處理\r\n                console.log(error);\r\n            });\r\n    },\r\n\r\n\r\n    computed: {\r\n        ProductsTable() {\r\n            return this.ProductsTable.filter(searchResult => searchResult.title.match(this.titleSearch));\r\n            // return: 一定要有回傳的值\r\n            // this.ProductsTable: 查ProductsTable\r\n            // filter: 篩選\r\n            // searchResult: 搜尋結果            \r\n            // \r\n            // match: 符合\r\n            // this.titleSearch: 輸入值titleSearch\r\n        }\r\n        \r\n    },\r\n    \r\n};\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n</script>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}