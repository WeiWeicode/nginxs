{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nexport default {\n  data() {\n    return {\n      // isShow: true\n      ProductsTable: [],\n      ord_total: '',\n      ord_phone: '',\n      ord_address: '',\n      ord_payment: '',\n      ord_byusername: '',\n      ord_byid: '',\n      ord_state: ''\n    };\n  },\n  mounted: function () {\n    let ProductsTable = JSON.parse(localStorage.getItem('ShoppingCart'));\n    this.ProductsTable = ProductsTable;\n    // 取得localStorage資料\n    this.ord_total = this.ProductsTable.reduce((total, item) => total + item.price, 0);\n    // 計算總金額\n    let user = JSON.parse(localStorage.getItem('user'));\n    this.ord_byusername = user.username;\n    this.ord_byid = user.id;\n    // byid: 取得localStorage user裡的id\n  },\n\n  methods: {\n    goHome() {\n      this.$router.push('/');\n    }\n\n    // 付款按鈕\n    // \n  },\n\n  computed: {\n    shoppingCartBoolean() {\n      if (localStorage.getItem('ShoppingCart') == null) {\n        return false;\n      } else {\n        return true;\n      }\n\n      // 判斷localStorage ShopoingCart是否有資料 \n    }\n  }\n};","map":{"version":3,"mappings":";AAwGA,eAAe;EAEXA,IAAI,GAAG;IACH,OAAO;MACH;MACAC,aAAa,EAAE,EAAE;MAEjBC,SAAS,EAAE,EAAE;MACbC,SAAS,EAAE,EAAE;MACbC,WAAW,EAAE,EAAE;MACfC,WAAW,EAAE,EAAE;MACfC,cAAc,EAAE,EAAE;MAClBC,QAAQ,EAAE,EAAE;MACZC,SAAS,EAAE;IAGf;EAEJ,CAAC;EAEDC,OAAO,EAAE,YAAY;IACjB,IAAIR,aAAY,GAAIS,IAAI,CAACC,KAAK,CAACC,YAAY,CAACC,OAAO,CAAC,cAAc,CAAC;IACnE,IAAI,CAACZ,aAAY,GAAIA,aAAY;IACjC;IACA,IAAI,CAACC,SAAQ,GAAI,IAAI,CAACD,aAAa,CAACa,MAAM,CAAC,CAACC,KAAK,EAAEC,IAAI,KAAKD,KAAI,GAAIC,IAAI,CAACC,KAAK,EAAE,CAAC;IACjF;IACA,IAAIC,IAAG,GAAIR,IAAI,CAACC,KAAK,CAACC,YAAY,CAACC,OAAO,CAAC,MAAM,CAAC;IAClD,IAAI,CAACP,cAAa,GAAIY,IAAI,CAACC,QAAO;IAClC,IAAI,CAACZ,QAAO,GAAIW,IAAI,CAACE,EAAC;IACtB;EACJ,CAAC;;EAEDC,OAAO,EAAE;IACLC,MAAM,GAAG;MACL,IAAI,CAACC,OAAO,CAACC,IAAI,CAAC,GAAG;IACzB;;IAEA;IACA;EAEJ,CAAC;;EAGDC,QAAQ,EAAE;IACNC,mBAAmB,GAAG;MAIlB,IAAId,YAAY,CAACC,OAAO,CAAC,cAAc,KAAK,IAAI,EAAE;QAC9C,OAAO,KAAI;MACf,OAAO;QACH,OAAO,IAAG;MACd;;MAEA;IACJ;EAEJ;AAGJ","names":["data","ProductsTable","ord_total","ord_phone","ord_address","ord_payment","ord_byusername","ord_byid","ord_state","mounted","JSON","parse","localStorage","getItem","reduce","total","item","price","user","username","id","methods","goHome","$router","push","computed","shoppingCartBoolean"],"sourceRoot":"","sources":["D:\\sshstudiocode\\Vue\\vue-3-authentication-jwt-shoppingcart\\src\\components\\package\\Payment.vue"],"sourcesContent":["<template>\r\n    <!-- 購物車轉付款的頁面 -->\r\n    <h1>結帳</h1>\r\n\r\n\r\n\r\n    <div class=\"wrap\">\r\n        <div v-if=\"!shoppingCartBoolean\">\r\n            <!-- 跳警告視窗並返回HOME -->\r\n            <div class=\"modal-body\">\r\n            <p>購物車為空，請先選購商品</p>\r\n        </div>\r\n        <div class=\"modal-footer\">\r\n            <button type=\"button\" class=\"btn btn-primary\" @click=\"goHome\">返回首頁</button>\r\n        </div>\r\n        </div>\r\n        \r\n    \r\n\r\n    <div v-show=\"shoppingCartBoolean\">\r\n\r\n        <div>\r\n            <table class=\"table table-striped table-bordered\">\r\n                <thead>\r\n                    <tr>\r\n                        <th>商品名稱</th>\r\n                        <th>商品價格</th>\r\n                    </tr>\r\n                </thead>\r\n                <tbody>\r\n                    <tr v-for=\"item in ProductsTable\" :key=\"item.id\">\r\n                        <td>{{ item.title }}</td>\r\n                        <td>{{ item.price }}</td>\r\n                    </tr>\r\n                </tbody>\r\n            </table>\r\n        </div>\r\n\r\n\r\n        <div class=\"form-group\">\r\n            <div class=\"alert alert-success\">\r\n            <label for=\"total\">總金額:{{ord_total}}</label>\r\n        </div>\r\n        </div>\r\n\r\n        <div class=\"form-group\">\r\n            <label for=\"byusername\">收件人</label>\r\n            <input type=\"text\" class=\"form-control\" id=\"byusername\" placeholder=\"請輸入收件人\" v-model=\"ord_byusername\">\r\n        </div>\r\n\r\n        <div class=\"form-group\">\r\n            <label for=\"phone\">電話</label>\r\n            <input type=\"tel\" class=\"form-control\" id=\"phone\" placeholder=\"請輸入電話\" v-model=\"ord_phone\">\r\n        </div>\r\n\r\n        <div class=\"form-group\">\r\n            <label for=\"address\">地址</label>\r\n            <input type=\"text\" class=\"form-control\" id=\"address\" placeholder=\"請輸入地址\" v-model=\"ord_address\">\r\n        </div>\r\n\r\n        <div class=\"form-group\">\r\n            <!-- 單選式付款方式(radio) 信用卡,ATM,貨到付款-->\r\n            <label for=\"payment\">付款方式</label>\r\n            <div class=\"form-check\">\r\n                <input class=\"form-check-input\" type=\"radio\" name=\"payment\" id=\"payment1\" value=\"信用卡\"\r\n                    v-model=\"ord_payment\">\r\n                <label class=\"form-check-label\" for=\"payment1\">\r\n                    信用卡\r\n                </label>\r\n            </div>\r\n            <div class=\"form-check\">\r\n                <input class=\"form-check-input\" type=\"radio\" name=\"payment\" id=\"payment2\" value=\"ATM\"\r\n                    v-model=\"ord_payment\">\r\n                <label class=\"form-check-label\" for=\"payment2\">\r\n                    ATM\r\n                </label>\r\n            </div>\r\n            <div class=\"form-check\">\r\n                <input class=\"form-check-input\" type=\"radio\" name=\"payment\" id=\"payment3\" value=\"貨到付款\"\r\n                    v-model=\"ord_payment\">\r\n                <label class=\"form-check-label\" for=\"payment3\">\r\n                    貨到付款\r\n                </label>\r\n            </div>\r\n        </div>\r\n        <!-- 付款按鈕 -->\r\n        <button type=\"submit\" class=\"btn btn-primary\" @click=\"payment\">付款</button>\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n    </div>\r\n</div>\r\n\r\n</template>\r\n\r\n\r\n<script>\r\n\r\nexport default {\r\n\r\n    data() {\r\n        return {\r\n            // isShow: true\r\n            ProductsTable: [],\r\n\r\n            ord_total: '',\r\n            ord_phone: '',\r\n            ord_address: '',\r\n            ord_payment: '',\r\n            ord_byusername: '',\r\n            ord_byid: '',\r\n            ord_state: ''\r\n\r\n\r\n        }\r\n\r\n    },\r\n\r\n    mounted: function () {\r\n        let ProductsTable = JSON.parse(localStorage.getItem('ShoppingCart'))\r\n        this.ProductsTable = ProductsTable\r\n        // 取得localStorage資料\r\n        this.ord_total = this.ProductsTable.reduce((total, item) => total + item.price, 0)\r\n        // 計算總金額\r\n        let user = JSON.parse(localStorage.getItem('user'))\r\n        this.ord_byusername = user.username\r\n        this.ord_byid = user.id\r\n        // byid: 取得localStorage user裡的id\r\n    },\r\n\r\n    methods: {\r\n        goHome() {\r\n            this.$router.push('/')\r\n        },\r\n\r\n        // 付款按鈕\r\n        // \r\n\r\n    },\r\n\r\n\r\n    computed: {\r\n        shoppingCartBoolean() {\r\n\r\n\r\n\r\n            if (localStorage.getItem('ShoppingCart') == null) {\r\n                return false\r\n            } else {\r\n                return true\r\n            }\r\n\r\n            // 判斷localStorage ShopoingCart是否有資料 \r\n        }\r\n\r\n    },\r\n\r\n\r\n}\r\n\r\n</script>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}