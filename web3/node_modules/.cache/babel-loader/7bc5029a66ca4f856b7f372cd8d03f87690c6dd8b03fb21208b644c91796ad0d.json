{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from \"axios\";\nimport qs from \"qs\";\nexport default {\n  data() {\n    return {\n      // isShow: true\n      ProductsTable: [],\n      ord_total: '',\n      ord_phone: '',\n      ord_address: '',\n      ord_payment: '',\n      ord_byusername: '',\n      ord_byid: '',\n      ord_state: '',\n      testrempid: '',\n      tempShppingCart: [],\n      tempres: [],\n      data: []\n    };\n  },\n  mounted: function () {\n    let ProductsTable = JSON.parse(localStorage.getItem('ShoppingCart'));\n    this.ProductsTable = ProductsTable;\n    // 取得localStorage資料\n    this.ord_total = this.ProductsTable.reduce((total, item) => total + item.price, 0);\n    // 計算總金額\n    let user = JSON.parse(localStorage.getItem('user'));\n    this.ord_byusername = user.username;\n    this.ord_byid = user.id;\n    // byid: 取得localStorage user裡的id\n  },\n\n  methods: {\n    goHome() {\n      this.$router.push('/');\n    },\n    Paying() {\n      // 將local資料轉成json格式\n\n      let order = {\n        ord_total: this.ord_total,\n        ord_phone: this.ord_phone,\n        ord_address: this.ord_address,\n        ord_payment: this.ord_payment,\n        ord_byusername: this.ord_byusername,\n        ord_byid: this.ord_byid,\n        ord_state: this.ord_state\n      };\n      console.log(order);\n      // 將local資料傳到orders\n\n      axios.post('http://192.168.68.60:8082/api/products/addOrder', order).then(res => {\n        console.log(res);\n        this.tempres = res.data;\n        // 取得後端回傳的訂單id\n        localStorage.setItem('tempres', JSON.stringify(this.tempres));\n        // 把this.tempres存到Local\n        // axios.get('http://192.168.68.60:8082/api/products/allOrders')\n        //     .then((res) => {\n        //         console.log(res)\n        //         // 將local資料傳到orderproducts,要對應到訂單id\n        //         axios.post('http://192.168.68.60:8082/api/products/addOrderProduct/' + res.data.id)\n        //             .then((res) => {\n        //                 console.log(res)\n        //             })\n        //             .catch((err) => {\n        //                 console.log(err)\n        //             })\n        //     })\n        //     .catch((err) => {\n        //         console.log(err)\n        //     })\n      }).catch(err => {\n        console.log(err);\n      });\n      let data = this.ProductsTable.map(item => {\n        return {\n          ord_product_id: item.title,\n          ord_product_title: item.price,\n          ord_product_price: item.id\n        };\n      });\n      return this.data = data;\n\n      // axios.post('http://192.168.68.60:8082/api/products/addOrderProduct/')\n\n      // axios.post('http://192.168.68.60:8082/api/products/addOrderProductid',{headers: { 'content-type': 'x-www-form-urlencoded'}})\n\n      // 用迴圈把每筆productsTable資料axios post到orderproducts\n\n      // 付款按鈕 (總共5秒處理)\n      // 1.將local資料傳到orders後端 (axios post orders http://192.168.68.60:8082/api/products/addOrder)\n      // 2.取得後端回傳的訂單id,存到local\n      // 3.將local資料傳到orderproducts,要對應到訂單id (axios post orderproducts http://192.168.68.60:8082/api/products/addOrderProduct/:id)\n      // 4.將local資料刪除\n      // 5.畫面跳轉到訂單頁面\n    },\n\n    testPaying() {\n\n      // 取出ProductsTable的id,title,price並轉成json格式\n      // let data = this.ProductsTable.map((item) => {\n      //     return {\n      //         opd_title: item.title,\n      //         opd_price: item.price,\n      //         opd_pid: item.id,\n      //     }\n      // })\n\n      // return this.data = data\n\n      // axios.post('http://192.168.68.60:8082/api/products/batchAddOrderproductID', data)\n      //     .then((res) => {\n      //         console.log(res)\n      //     })\n      //     .catch((err) => {\n      //         console.log(err)\n      //     })\n    }\n  },\n  computed: {\n    shoppingCartBoolean() {\n      if (localStorage.getItem('ShoppingCart') == null) {\n        return false;\n      } else {\n        return true;\n      }\n\n      // 判斷localStorage ShopoingCart是否有資料 \n    }\n  }\n};","map":{"version":3,"mappings":";AAuGA,OAAOA,KAAI,MAAO,OAAO;AACzB,OAAOC,EAAC,MAAO,IAAI;AAEnB,eAAe;EAEXC,IAAI,GAAG;IACH,OAAO;MACH;MACAC,aAAa,EAAE,EAAE;MAEjBC,SAAS,EAAE,EAAE;MACbC,SAAS,EAAE,EAAE;MACbC,WAAW,EAAE,EAAE;MACfC,WAAW,EAAE,EAAE;MACfC,cAAc,EAAE,EAAE;MAClBC,QAAQ,EAAE,EAAE;MACZC,SAAS,EAAE,EAAE;MACbC,UAAU,EAAE,EAAE;MAEdC,eAAe,EAAE,EAAE;MACnBC,OAAO,EAAE,EAAE;MAEXX,IAAI,EAAE;IAIV;EAEJ,CAAC;EAEDY,OAAO,EAAE,YAAY;IACjB,IAAIX,aAAY,GAAIY,IAAI,CAACC,KAAK,CAACC,YAAY,CAACC,OAAO,CAAC,cAAc,CAAC;IACnE,IAAI,CAACf,aAAY,GAAIA,aAAY;IACjC;IACA,IAAI,CAACC,SAAQ,GAAI,IAAI,CAACD,aAAa,CAACgB,MAAM,CAAC,CAACC,KAAK,EAAEC,IAAI,KAAKD,KAAI,GAAIC,IAAI,CAACC,KAAK,EAAE,CAAC;IACjF;IACA,IAAIC,IAAG,GAAIR,IAAI,CAACC,KAAK,CAACC,YAAY,CAACC,OAAO,CAAC,MAAM,CAAC;IAClD,IAAI,CAACV,cAAa,GAAIe,IAAI,CAACC,QAAO;IAClC,IAAI,CAACf,QAAO,GAAIc,IAAI,CAACE,EAAC;IACtB;EAEJ,CAAC;;EAEDC,OAAO,EAAE;IACLC,MAAM,GAAG;MACL,IAAI,CAACC,OAAO,CAACC,IAAI,CAAC,GAAG;IACzB,CAAC;IAGDC,MAAM,GAAG;MAEL;;MAEA,IAAIC,KAAI,GAAI;QACR3B,SAAS,EAAE,IAAI,CAACA,SAAS;QACzBC,SAAS,EAAE,IAAI,CAACA,SAAS;QACzBC,WAAW,EAAE,IAAI,CAACA,WAAW;QAC7BC,WAAW,EAAE,IAAI,CAACA,WAAW;QAC7BC,cAAc,EAAE,IAAI,CAACA,cAAc;QACnCC,QAAQ,EAAE,IAAI,CAACA,QAAQ;QACvBC,SAAS,EAAE,IAAI,CAACA;MACpB;MACAsB,OAAO,CAACC,GAAG,CAACF,KAAK;MACjB;;MAEA/B,KAAK,CAACkC,IAAI,CAAC,iDAAiD,EAAEH,KAAK,EAC9DI,IAAI,CAAEC,GAAG,IAAK;QACXJ,OAAO,CAACC,GAAG,CAACG,GAAG;QACf,IAAI,CAACvB,OAAM,GAAIuB,GAAG,CAAClC,IAAG;QACtB;QACAe,YAAY,CAACoB,OAAO,CAAC,SAAS,EAAEtB,IAAI,CAACuB,SAAS,CAAC,IAAI,CAACzB,OAAO,CAAC;QAC5D;QACA;QACA;QACA;QACA;QACA;QACA;QACA;QACA;QACA;QACA;QACA;QACA;QACA;QACA;QACA;MACJ,CAAC,EACA0B,KAAK,CAAEC,GAAG,IAAK;QACZR,OAAO,CAACC,GAAG,CAACO,GAAG;MACnB,CAAC;MAEL,IAAItC,IAAG,GAAI,IAAI,CAACC,aAAa,CAACsC,GAAG,CAAEpB,IAAI,IAAK;QACxC,OAAO;UACHqB,cAAc,EAAErB,IAAI,CAACsB,KAAK;UAC1BC,iBAAiB,EAAEvB,IAAI,CAACC,KAAK;UAC7BuB,iBAAiB,EAAExB,IAAI,CAACI;QAE5B;MACJ,CAAC;MAED,OAAO,IAAI,CAACvB,IAAG,GAAIA,IAAG;;MAGtB;;MAGA;;MAGA;;MAEA;MACA;MACA;M