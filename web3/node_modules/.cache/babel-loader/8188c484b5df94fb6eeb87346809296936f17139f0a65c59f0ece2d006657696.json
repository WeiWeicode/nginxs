{"ast":null,"code":"import { computed } from \"@vue/reactivity\";\nimport axios from \"axios\";\n// 引用axios\n\nexport default {\n  data() {\n    return {\n      // title: 'test2',\n      // price: \"\",\n      // description: \"\",\n      // published: \"\",\n      // image: \"\",\n      titleSearch: \"\",\n      ProductsTable: null,\n      path: \"http://192.168.68.60:8082/\"\n      // paht: 照片路徑使用\n    };\n  },\n\n  mounted: function () {\n    axios.get(\"http://192.168.68.60:8082/api/products/allProducts\")\n    // 發出取得請求\n    .then(response => {\n      // 取得回應後的處理\n      console.log(response);\n      // 這裡的response是API回傳的資料\n      this.ProductsTable = response.data;\n    }).catch(error => {\n      // 發生錯誤時的處理\n      console.log(error);\n    });\n  },\n  computed: {\n    titleSearch: function () {\n      var Products = this.ProductsTable;\n      var titleSearch = this.titleSearch;\n      if (!titleSearch) {\n        return Products;\n      }\n      searchString = titleSearch.trim().toLowerCase();\n      Products = Products.filter(function (item) {\n        if (item.title.toLowerCase().indexOf(titleSearch) !== -1) {\n          return item;\n        }\n      });\n      return Products;\n    }\n  }\n};","map":{"version":3,"mappings":"AAwBA,SAASA,QAAO,QAAS,iBAAiB;AAE1C,OAAOC,KAAI,MAAO,OAAO;AACzB;;AAIA,eAAe;EACXC,IAAI,GAAG;IACH,OAAO;MACH;MACA;MACA;MACA;MACA;MACAC,WAAW,EAAE,EAAE;MACfC,aAAa,EAAE,IAAI;MACnBC,IAAI,EAAE;MACN;IAEJ,CAAC;EAEL,CAAC;;EAEDC,OAAO,EAAE,YAAY;IACjBL,KAAI,CACCM,GAAG,CAAC,oDAAoD;IACzD;IAAA,CACCC,IAAI,CAAEC,QAAQ,IAAK;MAChB;MACAC,OAAO,CAACC,GAAG,CAACF,QAAQ,CAAC;MACrB;MACA,IAAI,CAACL,aAAY,GAAIK,QAAQ,CAACP,IAAI;IACtC,CAAC,EACAU,KAAK,CAAEC,KAAK,IAAK;MACd;MACAH,OAAO,CAACC,GAAG,CAACE,KAAK,CAAC;IACtB,CAAC,CAAC;EACV,CAAC;EAEDb,QAAQ,EAAE;IACNG,WAAW,EAAE,YAAY;MACrB,IAAIW,QAAO,GAAI,IAAI,CAACV,aAAa;MACjC,IAAID,WAAU,GAAI,IAAI,CAACA,WAAU;MAEjC,IAAI,CAACA,WAAW,EAAE;QACd,OAAOW,QAAQ;MACnB;MAEAC,YAAW,GAAIZ,WAAW,CAACa,IAAI,EAAE,CAACC,WAAW,EAAE;MAE/CH,QAAO,GAAIA,QAAQ,CAACI,MAAM,CAAC,UAAUC,IAAI,EAAE;QACvC,IAAIA,IAAI,CAACC,KAAK,CAACH,WAAW,EAAE,CAACI,OAAO,CAAClB,WAAW,MAAM,CAAC,CAAC,EAAE;UACtD,OAAOgB,IAAI;QACf;MACJ,CAAC;MACD,OAAOL,QAAQ;IAEnB;EAEJ;AAGJ,CAAC","names":["computed","axios","data","titleSearch","ProductsTable","path","mounted","get","then","response","console","log","catch","error","Products","searchString","trim","toLowerCase","filter","item","title","indexOf"],"sourceRoot":"","sources":["D:\\sshstudiocode\\Vue\\vue-3-authentication-jwt\\src\\components\\package\\Card.vue"],"sourcesContent":["<template>\r\n    <input type=\"text\" class=\"form-control\" v-model=\"titleSearch\" pl aceholder=\"Search\">\r\n\r\n    <div class=\"row row-cols-1 row-cols-md-3 g-4\">\r\n        <div class=\"col\" v-for=\"product in ProductsTable\" v-bind:key=\"product.id\">\r\n            <div class=\"card h-100\" style=\"width: 15rem;\">\r\n                <img v-bind:src=\"path + product.image\" class=\"card-img-top\" alt=\"\">\r\n                <!-- `http://localhost:3000/${productImage}` -->\r\n                <div class=\"card-body\">\r\n                    <h5 class=\"card-title\">{{ product.title }}</h5>\r\n                    <p class=\"card-text\">{{ product.description }}</p>\r\n                    <p class=\"card-text\">{{ product.price }}</p>\r\n                    <p class=\"card-text\">{{ product.published }}</p>\r\n\r\n                </div>\r\n                <div class=\"btn-group\" role=\"group\" aria-label=\"Basic mixed styles example\">\r\n                    <button type=\"button\" class=\"btn btn-success\">購物車</button>\r\n                </div>\r\n            </div>\r\n        </div>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\nimport { computed } from \"@vue/reactivity\";\r\n\r\nimport axios from \"axios\";\r\n// 引用axios\r\n\r\n\r\n\r\nexport default {\r\n    data() {\r\n        return {\r\n            // title: 'test2',\r\n            // price: \"\",\r\n            // description: \"\",\r\n            // published: \"\",\r\n            // image: \"\",\r\n            titleSearch: \"\",\r\n            ProductsTable: null,\r\n            path: \"http://192.168.68.60:8082/\"\r\n            // paht: 照片路徑使用\r\n\r\n        };\r\n\r\n    },\r\n\r\n    mounted: function () {\r\n        axios\r\n            .get(\"http://192.168.68.60:8082/api/products/allProducts\")\r\n            // 發出取得請求\r\n            .then((response) => {\r\n                // 取得回應後的處理\r\n                console.log(response);\r\n                // 這裡的response是API回傳的資料\r\n                this.ProductsTable = response.data;\r\n            })\r\n            .catch((error) => {\r\n                // 發生錯誤時的處理\r\n                console.log(error);\r\n            });\r\n    },\r\n\r\n    computed: {\r\n        titleSearch: function () {\r\n            var Products = this.ProductsTable;\r\n            var titleSearch = this.titleSearch\r\n\r\n            if (!titleSearch) {\r\n                return Products;\r\n            }\r\n\r\n            searchString = titleSearch.trim().toLowerCase();\r\n\r\n            Products = Products.filter(function (item) {\r\n                if (item.title.toLowerCase().indexOf(titleSearch) !== -1) {\r\n                    return item;\r\n                }\r\n            })\r\n            return Products;\r\n\r\n        }\r\n\r\n    }\r\n    \r\n    \r\n};\r\n</script>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}