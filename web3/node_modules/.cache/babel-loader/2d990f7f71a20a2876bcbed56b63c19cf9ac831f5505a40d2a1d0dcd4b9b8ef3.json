ion () {\r\n            var Products = this.ProductsTable;\r\n            var ProductsSearch = this.ProductsSearch\r\n\r\n            if (!ProductsSearch) {\r\n                return Products;\r\n            }\r\n\r\n            searchString = ProductsSearch.trim().toLowerCase();\r\n\r\n            \r\n\r\n\r\n        }\r\n\r\n    }\r\n    \r\n    \r\n};\r\n</script>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}