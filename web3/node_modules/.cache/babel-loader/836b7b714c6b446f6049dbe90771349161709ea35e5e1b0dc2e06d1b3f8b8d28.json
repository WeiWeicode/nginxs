n\r\n                <td>{{ item.image }}</td>\r\n                <td>{{ item.description }}</td>\r\n\r\n                <td>{{ item.published }}</td>\r\n                <td>{{ item.updatedAt }}</td>\r\n                <td>{{ item.createdAt }}</td>\r\n                <td>\r\n                    <button type=\"button\" class=\"btn btn-primary\" data-toggle=\"modal\" data-target=\"#exampleModal\"\r\n                        @click=\"updataproducts\">\r\n                        修改\r\n                    </button>\r\n                    <!-- 修改彈出視窗 引用Updataproducts.vue面板 -->\r\n                </td>\r\n            </tr>\r\n        </tbody>\r\n    </table>\r\n\r\n    <!-- Button trigger modal -->\r\n    <button type=\"button\" class=\"btn btn-primary\" data-toggle=\"modal\" data-target=\"#exampleModal\">\r\n        Launch demo modal\r\n    </button>\r\n\r\n    <!-- Modal -->\r\n    <div class=\"modal fade\" id=\"exampleModal\" tabindex=\"-1\" aria-labelledby=\"exampleModalLabel\" aria-hidden=\"true\">\r\n        <div class=\"modal-dialog\">\r\n            <div class=\"modal-content\">\r\n                <div class=\"modal-header\">\r\n                    <h5 class=\"modal-title\" id=\"exampleModalLabel\">Modal title</h5>\r\n                    <button type=\"button\" class=\"close\" data-dismiss=\"modal\" aria-label=\"Close\">\r\n                        <span aria-hidden=\"true\">&times;</span>\r\n                    </button>\r\n                </div>\r\n                <div class=\"modal-body\">\r\n                    {{ products }}}\r\n                </div>\r\n                <div class=\"modal-footer\">\r\n                    <button type=\"button\" class=\"btn btn-secondary\" data-dismiss=\"modal\">Close</button>\r\n                    <button type=\"button\" class=\"btn btn-primary\">Save changes</button>\r\n                </div>\r\n            </div>\r\n        </div>\r\n    </div>\r\n\r\n</template>\r\n    \r\n<script>\r\nimport axios from 'axios';\r\nimport Updataproducts from './Updataproducts.vue';\r\n\r\nexport default {\r\n    data() {\r\n        return {\r\n            products: [],\r\n\r\n\r\n\r\n        }\r\n    },\r\n\r\n    mounted:\r\n        function () {\r\n            axios.get(\"http://192.168.68.60:8082/api/products/allProducts\")\r\n                .then((res) => {\r\n                    this.products = res.data\r\n                    console.log(this.products)\r\n                })\r\n                .catch((err) => {\r\n                    console.log(err)\r\n                })\r\n\r\n        },\r\n\r\n\r\n    computed: {\r\n\r\n    },\r\n\r\n\r\n\r\n}\r\n\r\n</script>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}