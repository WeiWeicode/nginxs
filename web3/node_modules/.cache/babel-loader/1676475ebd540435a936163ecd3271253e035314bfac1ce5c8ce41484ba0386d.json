{"ast":null,"code":"import axios from 'axios';\nimport Updataproducts from './Updataproducts.vue';\nexport default {\n  components: {\n    Updataproducts\n  },\n  data() {\n    return {\n      products: [],\n      path: \"http://192.168.68.60:8082/\"\n    };\n  },\n  mounted: function () {\n    axios.get(\"http://192.168.68.60:8082/api/products/allProducts\").then(res => {\n      this.products = res.data;\n      console.log(this.products);\n    }).catch(err => {\n      console.log(err);\n    });\n  },\n  methods: {\n    // updataproducts:item寫入localStorage\n    updataproducts(item) {\n      localStorage.setItem('item', JSON.stringify(item));\n      // 刷新vue computed\n      this.$forceUpdate();\n    }\n  },\n  computed: {}\n};","map":{"version":3,"mappings":"AAqEA,OAAOA,KAAI,MAAO,OAAO;AACzB,OAAOC,cAAa,MAAO,sBAAsB;AAEjD,eAAe;EACXC,UAAU,EAAE;IACRD;EACJ,CAAC;EACDE,IAAI,GAAG;IACH,OAAO;MACHC,QAAQ,EAAE,EAAE;MAGZC,IAAI,EAAE;IAEV;EACJ,CAAC;EAEDC,OAAO,EACH,YAAY;IACRN,KAAK,CAACO,GAAG,CAAC,oDAAoD,EACzDC,IAAI,CAAEC,GAAG,IAAK;MACX,IAAI,CAACL,QAAO,GAAIK,GAAG,CAACN,IAAG;MACvBO,OAAO,CAACC,GAAG,CAAC,IAAI,CAACP,QAAQ;IAC7B,CAAC,EACAQ,KAAK,CAAEC,GAAG,IAAK;MACZH,OAAO,CAACC,GAAG,CAACE,GAAG;IACnB,CAAC;EAET,CAAC;EAELC,OAAO,EAAE;IACL;IACAC,cAAc,CAACC,IAAI,EAAE;MAEjBC,YAAY,CAACC,OAAO,CAAC,MAAM,EAAEC,IAAI,CAACC,SAAS,CAACJ,IAAI,CAAC,CAAC;MAClD;MACA,IAAI,CAACK,YAAY,EAAE;IAGvB;EAIJ,CAAC;EAGDC,QAAQ,EAAE,CAGV;AAIJ","names":["axios","Updataproducts","components","data","products","path","mounted","get","then","res","console","log","catch","err","methods","updataproducts","item","localStorage","setItem","JSON","stringify","$forceUpdate","computed"],"sourceRoot":"","sources":["D:\\sshstudiocode\\Vue\\vue-3-authentication-jwt-shoppingcart\\src\\components\\moderatorpackage\\Showproducts.vue"],"sourcesContent":["<template>\r\n    <h1>產品列表</h1>\r\n    <hr>\r\n\r\n    <table class=\"table table-bordered\">\r\n        <thead>\r\n            <tr>\r\n                <th scope=\"col\">產品編號</th>\r\n                <th scope=\"col\">產品名稱</th>\r\n                <th scope=\"col\">產品價格</th>\r\n                <th scope=\"col\">產品圖片</th>\r\n                <th scope=\"col\">產品描述</th>\r\n\r\n                <th scope=\"col\">產品是否上架</th>\r\n                <th scope=\"col\">產品更新日期</th>\r\n                <th scope=\"col\">產品建立日期</th>\r\n                <th scope=\"col\"></th>\r\n            </tr>\r\n        </thead>\r\n        <tbody>\r\n            <tr v-for=\"(item, index) in products\" :key=\"item.id\">\r\n                <td>{{ item.id }}</td>\r\n                <td>{{ item.title }}</td>\r\n                <td>{{ item.price }}</td>\r\n                <td><img :src=\"path + item.image\" class=\"card-img-top\" alt=\"\"></td>\r\n                <!-- <img :src=\"path + item.image\" class=\"card-img-top\" alt=\"\"> -->\r\n\r\n                <td>{{ item.description }}</td>\r\n\r\n                <td>{{ item.published }}</td>\r\n                <td>{{ item.updatedAt }}</td>\r\n                <td>{{ item.createdAt }}</td>\r\n                <td>\r\n                    <button type=\"button\" class=\"btn btn-primary\" data-toggle=\"modal\" data-target=\"#exampleModal\"\r\n                        @click=\"updataproducts(item)\">\r\n                        修改\r\n                    </button>\r\n                    <!-- 修改彈出視窗 引用Updataproducts.vue面板 -->\r\n                </td>\r\n            </tr>\r\n        </tbody>\r\n    </table>\r\n\r\n\r\n\r\n    <!-- Modal -->\r\n    <div class=\"modal\" id=\"exampleModal\" tabindex=\"-1\" aria-labelledby=\"exampleModalLabel\" aria-hidden=\"true\">\r\n        <div class=\"modal-dialog\">\r\n            <div class=\"modal-content\">\r\n                <div class=\"modal-header\">\r\n                    <button type=\"button\" class=\"close\" data-dismiss=\"modal\" aria-label=\"Close\">\r\n                        <span aria-hidden=\"true\">&times;</span>\r\n                    </button>\r\n                </div>\r\n                <div class=\"modal-body\">\r\n                    <Updataproducts></Updataproducts>\r\n                </div>\r\n                <div class=\"modal-footer\">\r\n                    <button type=\"button\" class=\"btn btn-secondary\" data-dismiss=\"modal\">Close</button>\r\n                </div>\r\n            </div>\r\n        </div>\r\n    </div>\r\n\r\n\r\n\r\n</template>\r\n    \r\n<script>\r\nimport axios from 'axios';\r\nimport Updataproducts from './Updataproducts.vue';\r\n\r\nexport default {\r\n    components: {\r\n        Updataproducts,\r\n    },\r\n    data() {\r\n        return {\r\n            products: [],\r\n\r\n\r\n            path: \"http://192.168.68.60:8082/\"\r\n\r\n        }\r\n    },\r\n\r\n    mounted:\r\n        function () {\r\n            axios.get(\"http://192.168.68.60:8082/api/products/allProducts\")\r\n                .then((res) => {\r\n                    this.products = res.data\r\n                    console.log(this.products)\r\n                })\r\n                .catch((err) => {\r\n                    console.log(err)\r\n                })\r\n\r\n        },\r\n\r\n    methods: {\r\n        // updataproducts:item寫入localStorage\r\n        updataproducts(item) {\r\n\r\n            localStorage.setItem('item', JSON.stringify(item));\r\n            // 刷新vue computed\r\n            this.$forceUpdate();\r\n\r\n\r\n        },\r\n\r\n\r\n\r\n    },\r\n\r\n\r\n    computed: {\r\n\r\n\r\n    },\r\n\r\n\r\n\r\n}\r\n\r\n</script>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}